	.global INT1_vect
INT1_vect:
	in	r2, _SFR_IO_ADDR(SREG)
	push	r2
	sbic	_SFR_IO_ADDR(PIND), PIND2
	rjmp	notPushed_1
	clr	r16
pushed_1:
	ldi		r17, 0
	call	EEPROM_write
	ldi		r17, 1
	call	EEPROM_write
	ldi		r17, 2
	call	EEPROM_write
	ldi		r17, 3
	call	EEPROM_write
   	inc      r18
   	cpi      r18, 3
   	breq     reset_1
   	rjmp     notPushed_1
reset_1:
   	clr      r18
notPushed_1:
	pop		r2
	out		_SFR_IO_ADDR(SREG), r2
	reti
